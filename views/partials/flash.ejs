<% if( warning && warning.length ) {%>
<div
  class="alert alert-warning alert-dismissible fade show col-md-6 offset-3"
  role="alert"
>
  <strong>Warning!</strong> <%= warning %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if( success && success.length ) {%>
<div
  class="alert alert-success alert-dismissible fade show col-md-6 offset-3"
  role="alert"
>
  <strong>Success!</strong> <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<% } %> <% if( error && error.length ) {%>
<div
  class="alert alert-danger alert-dismissible fade show col-md-6 offset-3"
  role="alert"
>
  <strong>Error!</strong> <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
